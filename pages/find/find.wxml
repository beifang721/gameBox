<!--pages/find/find.wxml-->

<view>
  <block wx:for="{{findGameData}}" wx:key="findDataKey">
    <view class='find-title width702 f38'>{{item.findTitle}}</view>
    <view class='findLogo width702' style='background-image:url({{item.findData.miniAppImgUrl}})'>
      <view class='game-intro-bg'>
        <view class='left'>
          <image class='find-icon' src='{{item.findData.miniAppImgUrl}}'></image>
          <view class='left-view'>
            <view class='f30'>{{item.findData.miniAppName}}</view>
            <view class='f24'>{{item.findData.miniAppName2}}</view>
          </view>
        </view>
        <view class='right'>
          <button class='f36'>开始</button>
        </view>
      </view>
    </view>
  </block>
</view>
