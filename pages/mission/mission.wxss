/* pages/mission/mission.wxss */
@import "/template/missionTemplate.wxss"; 
page{
  background-color: #f5f5f5
}
.sign-BG{
  width: 100%;
  height: 455rpx;
  position: absolute;
  top: 0;
}
.sign-header{
  position: relative;
  z-index: 10;
  background-color: #fff;
  border-radius: 5px;
  width: 92%;
  margin: auto
}
.item-a-wrap{
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  justify-content: space-around;
  
}

.sgin-item{
  width: 148rpx;
  height: 160rpx;
  border: 1px solid #dcdcdc;
  border-radius: 2px;
}
.yisginNum{
  padding: 50rpx 30rpx;
}
.goldImg-wrap .signGold{
  width: 54rpx;
  height: 54rpx;
}
.goldImg-wrap{
  text-align: center;
}
.item-b-wrap{
  display: flex;
  display: -webkit-flex;
  justify-content: center;
}
.item-b-wrap .sgin-item{
  margin: 10px 5px;
}
.mission-wrap{
  margin-top: 30rpx;
  padding-top: 30rpx;
}