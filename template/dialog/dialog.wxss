.diaLog{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
  position: absolute;
  top: 0;
  z-index: 900;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
}
.login-bg{
  width: 528rpx;
  height: 382rpx;
  background-image: url("https://caituwang.yisougame.cn/source_material/gameBox/login-bg.png");
  background-size: 100% 100%;
  position: relative;
  display: flex;
  display: -webkit-flex;
  justify-content: center;
}
.login-btnWrap{
  position: absolute;
  bottom: 20rpx;
}
.login-btnWrap .f30{
  color: #444;
}
.login-btnWrap .f32{
  margin-top: 20rpx;
  color: #fff;
  background-color: #42a2f8;
  width: 450rpx;
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 3px;
  position: relative;
}
.positionBtn{
   height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  background-color: #42a2f8
}
/* 签到弹窗 start */
.sign-bg{
  width: 658rpx;
  height: 800rpx;
  background-image: url("https://caituwang.yisougame.cn/source_material/gameBox/global-sign-BG.png");
  background-size: 100% 100%;
  position: relative;
  margin: auto;
  z-index: 1000;
}
.position-all-wrap{
  position: absolute;
  bottom: 130rpx;
  left: 50%; 
  transform: translateX(-50%);
}
.item-a-wrap{
  display: flex;
  display: -webkit-flex
}
.sgin-item{
  width: 140rpx;
  height: 160rpx;
  border: 1px solid #dcdcdc;
  border-radius: 2px;
  margin: 15rpx 8rpx;
}
.yisginNum{
  padding: 50rpx 30rpx;
}
.goldImg-wrap .signGold{
  width: 54rpx;
  height: 54rpx;
}
.goldImg-wrap{
  text-align: center;
}
.item-b-wrap{
  display: flex;
  display: -webkit-flex;
  justify-content: center;
}
.global-close-btn{
  position: absolute;
  right:48rpx; 
  top: 30rpx;
}
.global-sign-btn{
  position:absolute;
  bottom:20rpx;
  width: 442rpx;
  height: 92rpx;
  text-align: center;
  line-height: 92rpx;
  color: #fff;
  background-color: #0272d4;
  border-radius: 50px;
  left: 50%;
  transform: translateX(-50%)
}
/* 签到弹窗 end */