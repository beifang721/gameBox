.mission-wrap{
  background-color: #fff;
}
.mission {
  width: 95%;
  margin:0 auto 0;

}
.mission-title{
  padding: 0 0 25rpx 0;
  border-bottom: 1px solid #ddd;
  margin-bottom: 20rpx;
}
.mission-item{
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 40rpx;
}
.mission-left{
  display: flex;
  display: -webkit-flex;
}
.mission-icon{
  width: 50rpx;
  height: 50rpx;
  margin: 0 30rpx 0 10rpx;
}
.left-b .f28{
  color: rgb(34, 34, 34)
}
.left-b .f20{
  color: rgb(134, 134, 134)
}
.reward-mission-btn{
  color: #fff;
  background-color: #41a2f8;
  width: 157rpx;
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  border-radius: 50px;
  margin-right: 15rpx;
  position: relative;
}
.miss-btnActive{
  border:1px solid #41a2f8;
  color: #41a2f8;
  background-color: #fff;
}
.inivtFirend{
  opacity: 0
}
.list_mo{
  margin-bottom: 0;
}